<?xml version="1.0" encoding="UTF-8"?>
<physeng>
    <name>PoisSolver</name>
    <version>0.1</version>
    <interpreter></interpreter> <!-- used for calling a script with python, java, etc. Leave blank otherwise -->
    <bin_path>poissolver</bin_path> <!-- relative to engines.xml -->
    <sim_params>
        <param gui_object_type="QComboBox" gui_object_name="combo_bcs">bcs</param>
        <param gui_object_type="QLineEdit" gui_object_name="le_resolution">resolution</param>
        <param gui_object_type="QLineEdit" gui_object_name="le_length">length</param>
        <param gui_object_type="QLineEdit" gui_object_name="le_max_error">max_error</param>
        <param gui_object_type="QComboBox" gui_object_name="combo_mode">mode</param>
        <param gui_object_type="QLineEdit" gui_object_name="le_low_pot">low_pot</param>
        <param gui_object_type="QLineEdit" gui_object_name="le_high_pot">high_pot</param>
        <param gui_object_type="QLineEdit" gui_object_name="le_steps">steps</param>
    </sim_params>
</physeng>
